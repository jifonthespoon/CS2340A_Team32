<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="450dp"
        android:layout_height="96dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/red_rectangle"
        tools:layout_editor_absoluteX="-20dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="360dp"
        android:layout_height="86dp"
        android:layout_marginTop="8dp"
        android:scaleX=".7"
        android:scaleY=".7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.019"
        app:srcCompat="@drawable/fix_a_plate" />


    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="448dp"
        android:layout_height="101dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:srcCompat="@drawable/nav_bar"
        tools:layout_editor_absoluteX="-14dp" />


    <ImageButton
        android:id="@+id/toHomePage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:background="@null"
        android:scaleX=".65"
        android:scaleY=".65"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.464"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.967"
        app:srcCompat="@drawable/red_home_nav_button" />

    <ImageButton
        android:id="@+id/toIngredientsPage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:background="@null"
        android:scaleX=".5"
        android:scaleY=".5"
        app:layout_constraintEnd_toStartOf="@+id/toShoppingPage"
        app:srcCompat="@drawable/ingredients_button"
        tools:layout_editor_absoluteY="785dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="88dp"
        android:text="Recipe"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        tools:layout_editor_absoluteY="865dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Shopping List"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        tools:layout_editor_absoluteY="865dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ingredients"
        tools:layout_editor_absoluteX="228dp"
        tools:layout_editor_absoluteY="865dp" />

    <ImageButton
        android:id="@+id/toShoppingPage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        android:scaleX=".5"
        android:scaleY=".5"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/shopping_list_button"
        tools:layout_editor_absoluteY="790dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Input"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="0.337"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="865dp" />

    <ImageButton
        android:id="@+id/toRecipePage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="60dp"
        android:background="@null"
        android:scaleX=".5"
        android:scaleY=".5"
        app:layout_constraintEnd_toStartOf="@+id/toIngredientsPage"
        app:srcCompat="@drawable/recipe_button"
        tools:layout_editor_absoluteY="795dp" />

    <ImageButton
        android:id="@+id/toInputPage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        android:scaleX=".5"
        android:scaleY=".5"
        app:layout_constraintEnd_toStartOf="@+id/toRecipePage"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/input_button"
        tools:layout_editor_absoluteY="790dp" />



</androidx.constraintlayout.widget.ConstraintLayout>




